        document.addEventListener('DOMContentLoaded', function () {
            const productGrid = document.getElementById('productGrid');
            const contactForm = document.getElementById('contactForm');
            const modal = document.getElementById('imageModal');
            const modalImg = document.getElementById('modalImage');
            const closeBtn = document.getElementsByClassName('close')[0];
            const menuToggle = document.getElementById('menuToggle');
            const mainMenu = document.getElementById('mainMenu');
            const menuOverlay = document.getElementById('menuOverlay');
            const closeMenuBtn = document.getElementById('closeMenuBtn');
            const formFeedback = document.getElementById('formFeedback');
            const formLoading = document.getElementById('formLoading');
            const cookieBanner = document.getElementById('cookieBanner');
            const acceptCookiesBtn = document.getElementById('acceptCookies');
            const rejectCookiesBtn = document.getElementById('rejectCookies');
            
            function checkCookiePreference() {
                const cookiePreference = getCookie('cookiePreference');
                
                if (cookiePreference === '') {
                    cookieBanner.classList.add('show');
                } else if (cookiePreference === 'accepted') {
                }
            }
            
            function setCookie(name, value, days) {
                let expires = "";
                if (days) {
                    const date = new Date();
                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                    expires = "; expires=" + date.toUTCString();
                }
                document.cookie = name + "=" + (value || "") + expires + "; path=/";
            }
            
            function getCookie(name) {
                const nameEQ = name + "=";
                const ca = document.cookie.split(';');
                for (let i = 0; i < ca.length; i++) {
                    let c = ca[i];
                    while (c.charAt(0) === ' ') c = c.substring(1, c.length);
                    if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
                }
                return '';
            }
            
            acceptCookiesBtn.addEventListener('click', function() {
                setCookie('cookiePreference', 'accepted', 365);
                cookieBanner.classList.remove('show');
            });
            
            rejectCookiesBtn.addEventListener('click', function() {
                setCookie('cookiePreference', 'rejected', 365);
                cookieBanner.classList.remove('show');
            });
            
            checkCookiePreference();
            
            menuToggle.addEventListener('click', function() {
                openMenu();
            });

            closeMenuBtn.addEventListener('click', function() {
                closeMenu();
            });

            menuOverlay.addEventListener('click', function() {
                closeMenu();
            });

            document.querySelectorAll('#mainMenu a').forEach(link => {
                link.addEventListener('click', function() {
                    closeMenu();
                });
            });

            function openMenu() {
                document.body.style.overflow = 'hidden';
                mainMenu.classList.add('show');
                menuOverlay.style.display = 'block';
                closeMenuBtn.style.display = 'block';
                
                document.querySelector('.menu-title').style.display = 'block';
                document.querySelector('.menu-social-links').style.display = 'flex';
            }

            function closeMenu() {
                document.body.style.overflow = 'auto';
                mainMenu.classList.remove('show');
                menuOverlay.style.display = 'none';
                closeMenuBtn.style.display = 'none';
                
                document.querySelector('.menu-title').style.display = 'none';
                document.querySelector('.menu-social-links').style.display = 'none';
            }

            window.openModal = function (imgSrc) {
                modal.style.display = "block";
                modalImg.src = imgSrc;
            }

            closeBtn.onclick = function () {
                modal.style.display = "none";
            }

            window.onclick = function (event) {
                if (event.target == modal) {
                    modal.style.display = "none";
                }
            }

            function validateForm() {
                let isValid = true;
                
                const name = document.getElementById('name');
                if (name.value.trim() === '' || name.value.trim().length < 3) {
                    name.parentElement.classList.add('error');
                    isValid = false;
                } else {
                    name.parentElement.classList.remove('error');
                }
                
                const email = document.getElementById('email');
                const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                if (!emailRegex.test(email.value.trim())) {
                    email.parentElement.classList.add('error');
                    isValid = false;
                } else {
                    email.parentElement.classList.remove('error');
                }
                
                const phone = document.getElementById('phone');
                const phoneRegex = /^(\d{10,11})$/;
                if (!phoneRegex.test(phone.value.trim().replace(/\D/g, ''))) {
                    phone.parentElement.classList.add('error');
                    isValid = false;
                } else {
                    phone.parentElement.classList.remove('error');
                }
                
                const message = document.getElementById('message');
                if (message.value.trim() === '' || message.value.trim().length < 10) {
                    message.parentElement.classList.add('error');
                    isValid = false;
                } else {
                    message.parentElement.classList.remove('error');
                }
                
                if (typeof grecaptcha !== 'undefined') {
                    const response = grecaptcha.getResponse();
                    if (response.length === 0) {
                        formFeedback.textContent = 'Por favor, complete o reCAPTCHA.';
                        formFeedback.classList.add('error');
                        formFeedback.style.display = 'block';
                        isValid = false;
                    }
                }
                
                return isValid;
            }

            if (contactForm) {
                contactForm.addEventListener('submit', function (e) {
                    e.preventDefault();
                    
                    formFeedback.style.display = 'none';
                    formFeedback.className = 'form-feedback';
                    
                    if (!validateForm()) {
                        return;
                    }
                    
                    formLoading.style.display = 'block';
                    
                    const formData = new FormData(contactForm);
                    
                    if (typeof grecaptcha !== 'undefined') {
                        const response = grecaptcha.getResponse();
                        formData.append('g-recaptcha-response', response);
                    }
                    
                    fetch('https://formsubmit.co/ajax/nckmembranacomercial@gmail.com', {
                        method: 'POST',
                        body: formData
                    })
                    .then(response => response.json())
                    .then(data => {
                        formLoading.style.display = 'none';
                        
                        if (data.success === 'true') {
                            formFeedback.textContent = 'Mensagem enviada com sucesso! Entraremos em contato em breve.';
                            formFeedback.classList.add('success');
                            formFeedback.style.display = 'block';
                            
                            contactForm.reset();
                            if (typeof grecaptcha !== 'undefined') {
                                grecaptcha.reset();
                            }
                            
                            formFeedback.scrollIntoView({ behavior: 'smooth' });
                        } else {
                            formFeedback.textContent = 'Ocorreu um erro ao enviar sua mensagem. Por favor, tente novamente mais tarde ou entre em contato por outro meio.';
                            formFeedback.classList.add('error');
                            formFeedback.style.display = 'block';
                        }
                    })
                    .catch(error => {
                        formLoading.style.display = 'none';
                        
                        formFeedback.textContent = 'Ocorreu um erro ao enviar sua mensagem. Por favor, tente novamente mais tarde ou entre em contato por outro meio.';
                        formFeedback.classList.add('error');
                        formFeedback.style.display = 'block';
                        console.error('Error:', error);
                    });
                });
            }

            const inputs = document.querySelectorAll('#contactForm input, #contactForm textarea');
            inputs.forEach(input => {
                input.addEventListener('input', function() {
                    if (this.value.trim() !== '') {
                        this.parentElement.classList.remove('error');
                    }
                });
            });

            let currentCategory = 'all';

            function loadProducts() {
                if (currentCategory === 'all') {
                    const categories = [...new Set(products.map(p => p.category))];
                    const filteredProducts = [];

                    categories.forEach(category => {
                        const firstProduct = products.find(p => p.category === category);
                        if (firstProduct) filteredProducts.push(firstProduct);
                    });

                    renderProducts(filteredProducts);
                } else {
                    const filteredProducts = products.filter(p => p.category === currentCategory);
                    renderProducts(filteredProducts);
                }
            }

            function renderProducts(productsToRender) {
                productGrid.innerHTML = productsToRender.map(product => `
                    <div class="product-card" data-category="${product.category}">
                        <div class="product-image">
                            <img src="${product.image}" alt="${product.name}" onclick="openModal('${product.image}')">
                        </div>
                        <div class="product-info">
                            <h3>${product.name}</h3>
                            <p>${product.description}</p>
                            
                            <ul class="product-features">
                                ${product.features.map(feature => `
                                    <li><i class="fas fa-check-circle"></i> ${feature}</li>
                                `).join('')}
                            </ul>
                            
                            <div class="product-contact">
                                <a href="https://wa.me/551158259390?text=Olá,%20gostaria%20de%20saber%20mais%20sobre%20o%20produto:%20${encodeURIComponent(product.name)}" 
                                   target="_blank" 
                                   class="product-contact-btn whatsapp">
                                    <i class="fab fa-whatsapp"></i> WhatsApp
                                </a>
                                <a href="https://www.facebook.com/profile.php?id=61576193393403" 
                                   target="_blank" 
                                   class="product-contact-btn facebook">
                                    <i class="fab fa-facebook"></i> Facebook
                                </a>
                                <a href="https://www.linkedin.com/in/nck-membrana-199038364/?trk=opento_sprofile_topcard" 
                                   target="_blank" 
                                   class="product-contact-btn linkedin">
                                    <i class="fab fa-linkedin"></i> LinkedIn
                                </a>
                                <a href="https://www.instagram.com/nck.membrana.comercial/" 
                                   target="_blank" 
                                   class="product-contact-btn instagram">
                                    <i class="fab fa-instagram"></i> Instagram
                                </a>
                            </div>
                        </div>
                    </div>
                `).join('');
            }

            const categoryBtns = document.querySelectorAll('.category-btn');

            categoryBtns.forEach(btn => {
                btn.addEventListener('click', function () {
                    categoryBtns.forEach(b => b.classList.remove('active'));
                    this.classList.add('active');

                    currentCategory = this.getAttribute('data-category');
                    loadProducts();
                });
            });

            loadProducts();
            
            const menuTitle = document.createElement('div');
            menuTitle.className = 'menu-title';
            menuTitle.textContent = 'Menu';
            menuTitle.style.display = 'none';
            mainMenu.insertBefore(menuTitle, mainMenu.firstChild);
            
            const menuSocialLinks = document.createElement('div');
            menuSocialLinks.className = 'menu-social-links';
            menuSocialLinks.innerHTML = `
                <a href="https://wa.me/551158259390" target="_blank"><i class="fab fa-whatsapp"></i></a>
                <a href="https://www.facebook.com/profile.php?id=61576193393403" target="_blank"><i class="fab fa-facebook"></i></a>
                <a href="https://www.instagram.com/nck.membrana.comercial/" target="_blank"><i class="fab fa-instagram"></i></a>
            `;
            menuSocialLinks.style.display = 'none';
            mainMenu.appendChild(menuSocialLinks);
        });
    </script>
</body>
</html>
